    "test": "vitest run"
  },
  "devDependencies": {
    "@bufbuild/buf": "^1.57.2",
    "@bufbuild/protoc-gen-es": "^2.9.0",
    "@types/node": "22.7.4",
    "eslint": "9.12.0",
    "rimraf": "6.0.1",
    "typescript": "5.6.3",
    "vitest": "^3.0.0"
  },
  "dependencies": {
    "@bufbuild/protobuf": "^2.9.0",
    "@connectrpc/connect": "2.1.0",
    "@connectrpc/connect-node": "2.1.0",
    "@connectrpc/connect-web": "2.1.0"
  }
}
